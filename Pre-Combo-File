#LECO_2021
Nitrate_LECO_2021 <-read_csv("../Data/Processed/LECO (TN-D07)/LECO_NEON_nitrate-surfacewater/NEON.D07.LECO.DP1.20033.001.112.100.015.NSW_15_minute.2021-09.basic.20221208T012753Z.csv") %>% 
